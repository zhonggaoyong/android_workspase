<?xml version="1.0" encoding="utf-8"?>
<set android:shareInterpolator="false"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate android:interpolator="@android:anim/accelerate_interpolator" android:duration="200" android:fromDegrees="0.0" android:toDegrees="-30.0" android:pivotX="50.0%" android:pivotY="100.0%" android:toYScale="0.0" />
    <set android:startOffset="200">
        <rotate android:interpolator="@android:anim/decelerate_interpolator" android:duration="200" android:fromDegrees="0.0" android:toDegrees="45.0" android:pivotX="50.0%" android:pivotY="100.0%" android:toYScale="0.0" />
    </set>
    <set android:startOffset="400">
        <rotate android:interpolator="@android:anim/accelerate_interpolator" android:duration="200" android:fromDegrees="0.0" android:toDegrees="30.0" android:pivotX="50.0%" android:pivotY="100.0%" android:toYScale="0.0" />
        <set android:startOffset="200">
            <rotate android:interpolator="@android:anim/accelerate_interpolator" android:duration="200" android:fromDegrees="0.0" android:toDegrees="-30.0" android:pivotX="50.0%" android:pivotY="100.0%" android:toYScale="0.0" />
        </set>
    </set>
</set>