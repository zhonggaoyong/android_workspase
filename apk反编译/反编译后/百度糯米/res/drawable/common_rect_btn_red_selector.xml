<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/order_btn_gray" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/order_btn_red_normal" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/order_btn_red_press" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/order_btn_red_normal" />
        </shape>
    </item>
</selector>