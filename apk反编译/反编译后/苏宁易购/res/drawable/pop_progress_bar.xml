<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#0fffffff" android:endColor="#0fffffff" android:angle="270.0" android:centerColor="#0fffffff" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff00c957" android:endColor="#ff00c957" android:angle="270.0" android:centerColor="#ff00c957" />
            </shape>
        </clip>
    </item>
</layer-list>