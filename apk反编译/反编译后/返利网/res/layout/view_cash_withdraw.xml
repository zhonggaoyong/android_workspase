            <com.fanli.android.view.TangFontTextView android:textSize="@dimen/text_size_medium" android:textColor="@color/text_color_grey_dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="提现金额" android:singleLine="true" android:layout_alignParentLeft="true" />
