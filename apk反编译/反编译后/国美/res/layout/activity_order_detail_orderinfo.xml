<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="#ffffffff" android:layout_width="fill_parent" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <View android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" />
    <RelativeLayout android:id="@id/od_orderinfo_title_layout" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/od_frame_padding" android:layout_marginTop="@dimen/od_frame_title_padding" android:layout_marginRight="@dimen/od_frame_padding">
        <TextView android:textSize="@dimen/od_font_15" android:textStyle="bold" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="订单信息" android:layout_alignParentLeft="true" android:layout_centerVertical="true" />
        <TextView android:textSize="@dimen/od_font_12" android:textColor="@color/yn_sub_title_color" android:id="@id/od_orderinfo_edit" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/od_modify" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
    </RelativeLayout>
    <RelativeLayout android:id="@id/od_orderinfo_body_layout" android:paddingTop="@dimen/od_frame_body_padding" android:paddingBottom="@dimen/od_frame_body_padding" android:visibility="visible" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/od_frame_padding" android:layout_marginRight="@dimen/od_frame_padding" android:layout_below="@id/od_orderinfo_title_layout">
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_id" android:layout_width="@dimen/od_textview_width" android:layout_height="wrap_content" android:text="订单编号：" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_id_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="" android:layout_toRightOf="@id/od_orderinfo_id" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_peisong" android:layout_width="@dimen/od_textview_width" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="配送单号：" android:layout_below="@id/od_orderinfo_id" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_peisong_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="" android:layout_toRightOf="@id/od_orderinfo_peisong" android:layout_below="@id/od_orderinfo_id" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_time" android:layout_width="@dimen/od_textview_width" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="下单时间：" android:layout_below="@id/od_orderinfo_peisong" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_time_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="" android:layout_toRightOf="@id/od_orderinfo_time" android:layout_below="@id/od_orderinfo_peisong" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_jifen" android:layout_width="@dimen/od_textview_width" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="订单赠积分：" android:layout_below="@id/od_orderinfo_time_data" android:layout_alignLeft="@id/od_orderinfo_time" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_orderinfo_jifen_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="" android:layout_toRightOf="@id/od_orderinfo_jifen" android:layout_below="@id/od_orderinfo_time_data" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/hint_text_color" android:id="@id/od_orderinfo_note" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="@string/od_orderinfo_note_string" android:layout_below="@id/od_orderinfo_jifen" />
    </RelativeLayout>
    <View android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" android:layout_below="@id/od_orderinfo_body_layout" />
</RelativeLayout>