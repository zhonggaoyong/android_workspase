<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="#ffffffff" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/od_model_margin_bottom"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <View android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" />
    <RelativeLayout android:id="@id/od_invoice_title_layout" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/od_frame_padding" android:layout_marginTop="@dimen/od_frame_title_padding" android:layout_marginRight="@dimen/od_frame_padding">
        <TextView android:textSize="@dimen/od_font_15" android:textStyle="bold" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="发票信息" android:layout_alignParentLeft="true" android:layout_centerVertical="true" />
        <TextView android:textSize="@dimen/od_font_12" android:textColor="@color/yn_sub_title_color" android:id="@id/od_invoice_edit" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/od_modify" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
    </RelativeLayout>
    <RelativeLayout android:id="@id/od_invoice_body_layout" android:paddingTop="@dimen/od_frame_body_padding" android:paddingBottom="@dimen/od_frame_body_padding" android:visibility="visible" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/od_frame_padding" android:layout_marginRight="@dimen/od_frame_padding" android:layout_below="@id/od_invoice_title_layout">
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_type" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="发票类型：" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:ellipsize="end" android:id="@id/od_invoice_type_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="" android:singleLine="true" android:layout_toRightOf="@id/od_invoice_type" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/link_text_color" android:id="@id/od_invoice_look" android:focusable="true" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="查看发票" android:layout_alignParentRight="true" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_header" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="发票抬头：" android:layout_below="@id/od_invoice_type" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:ellipsize="end" android:id="@id/od_invoice_header_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="" android:singleLine="true" android:layout_toRightOf="@id/od_invoice_header" android:layout_below="@id/od_invoice_type" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_content" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="发票内容：" android:layout_below="@id/od_invoice_header" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_content_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="" android:layout_toRightOf="@id/od_invoice_content" android:layout_below="@id/od_invoice_header" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_address" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="寄票地址：" android:layout_below="@id/od_invoice_content_data" />
        <TextView android:textSize="@dimen/od_font_13" android:textColor="@color/main_default_black_text_color" android:id="@id/od_invoice_address_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/od_frame_line_max_padding" android:text="" android:layout_toRightOf="@id/od_invoice_address" android:layout_below="@id/od_invoice_content_data" />
    </RelativeLayout>
    <View android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" android:layout_below="@id/od_invoice_body_layout" />
</RelativeLayout>