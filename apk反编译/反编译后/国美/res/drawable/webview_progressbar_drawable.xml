<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffe6e6e6" android:endColor="#ffe6e6e6" android:angle="90.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffef3030" android:endColor="#ffef3030" android:angle="90.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>